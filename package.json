{
    "name": "studhalter.dev",
    "version": "1.0.0",
    "description": "Personal website of Rainer Studhalter @ studhalter.dev",
    "keywords": ["sveltekit", "unocss", "iconify", "inlag", "paraglide-js"],
    "author": "Rainer Studhalter <rainer@studhalter.dev> (https://studhalter.dev)",
    "repository": {
        "type": "git",
        "url": "https://github.com/rainerstudhalter/studhalter.dev.git"
    },
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/rainerstudhalter/studhalter.dev/issues"
    },
    "homepage": "https://github.com/rainerstudhalter/studhalter.dev#readme",
    "type": "module",
    "scripts": {
        "dev": "vite dev --host",
        "debug": "dev --debug",
        "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide"
    },
    "devDependencies": {
        "@iconify/json": "^2.2.161",
        "@inlang/paraglide-js": "^1.0.0-prerelease.22",
        "@inlang/paraglide-sveltekit": "^0.6.12",
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/adapter-node": "^4.0.1",
        "@sveltejs/adapter-static": "^3.0.1",
        "@sveltejs/kit": "^2.5.2",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@types/node": "^20.11.24",
        "@unocss/extractor-svelte": "^0.59.4",
        "@unocss/transformer-directives": "^0.59.4",
        "@unocss/transformer-variant-group": "^0.59.4",
        "dayjs": "^1.11.10",
        "dotenv": "^16.4.5",
        "nodemailer": "^6.9.11",
        "qrcode": "^1.5.3",
        "sass": "^1.70.0",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "ulidx": "^2.3.0",
        "unocss": "^0.59.4",
        "vite": "^5.0.3",
        "wait": "^0.4.2",
        "zod-form-data": "^2.0.2"
    },
    "private": true
}
